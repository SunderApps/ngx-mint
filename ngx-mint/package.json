{
  "name": "@appartmint/ngx-mint",
  "version": "0.0.1",
  "sideEffects": false,
  "publishConfig": {
    "access": "private",
    "registry": "https://npm.pkg.github.com/@appartmint"
  },
  "scripts": {
    "prepare": "npm run build",
