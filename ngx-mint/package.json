    "test": "ng test",
    "lint": "ng lint"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@appartmint/mint": "^0.9.7"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
